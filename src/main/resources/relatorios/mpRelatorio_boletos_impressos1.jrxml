	<queryString>
		<![CDATA[SELECT
     mp_boleto_log.data_geracao AS data_geracao,
     mp_boleto_log.valor_documento AS valor_documento,
     mp_boleto_log.usuario_nome AS usuario_nome,
     mp_boleto_log.numero_documento AS numero_documento,
     mp_boleto_log.numero_oficio AS numero_oficio,
     mp_boleto_log.protocolo AS protocolo,
     mp_boleto_log.numero_guia AS numero_guia,
     mp_boleto_log.valor_cpmf AS valor_cpmf,
     mp_boleto_log.valor_tarifa AS valor_tarifa,
     mp_boleto_log.valor_leis AS valor_leis,
     mp_boleto_log.valor_cobrado AS valor_cobrado
FROM
     mp_boleto_log
WHERE
 	 (mp_boleto_log.data_geracao BETWEEN $P{data_inicio} and $P{data_fim}) AND (numero_oficio = $P{numero_oficio} AND $P{numero_oficio} != 'X')
ORDER BY
     protocolo,  numero_guia]]>
